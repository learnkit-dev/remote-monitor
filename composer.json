{
    "name": "learnkit/remote-monitor",
    "description": "A Laravel package for remote monitoring",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LearnKit\\RemoteMonitor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LearnKit\\RemoteMonitor\\RemoteMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}